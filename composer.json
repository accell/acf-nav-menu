{
  "name": "accell/acf-nav-menu",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "ACF field that allows you to select a nav menu.",
  "homepage": "https://github.com/accell/acf-nav-menu",
  "authors": [
    {
      "name": "Eric ColÃ³n",
      "email": "me@ericcolon.com",
      "homepage": "https://github.com/accell"
    }
  ],
  "keywords": [
    "acf",
    "acf-field",
    "wordpress",
    "wordpress-plugin"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Accell\\AcfNavMenu\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --exclude=PSR1.Methods.CamelCapsMethodName --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
