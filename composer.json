{
  "name": "accell/acf-nav-menu",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "ACF field that allows you to select a nav menu.",
  "keywords": [
    "acf",
    "acf-field",
    "wordpress",
    "wordpress-plugin"
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Accell\\AcfNavMenu\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --exclude=PSR1.Methods.CamelCapsMethodName --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
